calc = {}

def main():
	global calc
	
	ways(200)
	
	for keys, items in calc.items():
		print(keys, items)
	
	print(calc[200])
	
	
def ways(goal, m = 0):
	global calc
	coins = [200, 100, 50, 20, 10, 5, 2, 1]
	
	if goal in calc:
		print(goal, " is in calc as ", calc[goal])
		return calc[goal]
	
	if goal == 0:
		return m
	
	for coin in coins:
		if coin <= goal:
			print(goal, coin, goal-coin)
			num = ways(goal-coin, m + 1)
			calc[goal] += num
	
	
	
	
if __name__ == '__main__':
	main()
