import string

def main():
	
	with open('p059_cipher.txt', 'r') as file:
		key = file.read()
		
	keyList = key.split(",")
	for num in range(len(keyList)):
		keyList[num] = int(keyList[num])
#	print(keyList)
	
	letters = string.ascii_lowercase
	print(letters)
	
	for l1 in letters:
		for l2 in letters:
			for l3 in letters:
				solve = [ord(l1), ord(l2), ord(l3)]*(len(keyList)//3)
				print(solve)
				print()
	
if __name__ == "__main__":
	main()
