from math import sqrt


def main():

    for i in range(51):
        if sqrt(i) % 1 != 0:
            print(i, dio(i))


def dio(d):
    x = 1
    y = 1

    while True:
        if x**2 - d*y**2 == 1:
            return x, y
        elif x**2 - d*y**2 < 1:
            x += 1
        else:
            y += 1


if __name__ == '__main__':
    main()
